{
  "name": "endosul",
  "version": "1.0.0",
  "description": "desc",
  "engines": {
      "node": ">=8.9.4"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node src/index.js",
    "start": "npm run dev",
    "prod": "node dist/index.js",
    "_rm-build-prod": "rimraf dist",
    "_build-prod": "cross-env NODE_ENV=production babel src --out-dir dist --copy-files",
    "build-prod": "npm run _rm-build-prod && npm run _build-prod",
    "test": "cross-env NODE_ENV=test mocha --opts mocha.opts",
    "lint": "eslint src config test"
  },
  "dependencies": {
    "koa-smart": "^2.0.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "expect": "^23.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      "apidoc"
    ]
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clementB91/endosul.git"
  },
  "keywords": [
    "node",
    "heroku",
    "express"
  ],
  "author": "iLLo_B2OK1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/clementB91/endosul/issues"
  },
  "homepage": "https://github.com/clementB91/endosul#readme"
}
